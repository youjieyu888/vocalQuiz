<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.seclass.sdpvocabquiz">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddQuizActivity" />
        <activity android:name=".AddQuestionActivity" />
        <activity android:name=".AddIncorrectActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".RemoveQuiz" />
        <activity
            android:name=".PracticeActivity"
            android:label="@string/title_activity_practice" />
        <activity
            android:name=".QuizStatsActivity"
            android:label="@string/title_activity_quiz_stats" />
        <activity
            android:name=".ViewQuiz"
            android:label="@string/title_activity_view_quiz3" />
        <activity android:name=".RemoveViewQuiz" />

        <meta-data
            android:name="DATABASE"
            android:value="vocab_quiz.db" />
        <meta-data
            android:name="VERSION"
            android:value="4" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />

        <activity android:name=".ChooseQuizActivity" />
        <activity android:name=".ShowQuizResultActivity"></activity>
    </application>

</manifest>